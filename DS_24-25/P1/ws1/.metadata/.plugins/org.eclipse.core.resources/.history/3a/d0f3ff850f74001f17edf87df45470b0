/**
 * @author UO285176
 *
 */
public class Temperatura {
	double grados = 0;

	public Temperatura(double grados) {
		this.grados = 0;
	}
	
	public double getCelsius()
	{
		return grados;
	}
	
	public double getFahrenheit() {
		return this.grados * 1.8 + 32;
	}
}
