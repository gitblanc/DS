package instrucciones;

import main.Main;
import main.Pila;

public class JmpgInstruction implements Instruction {

	@Override
	public void execute(Pila pila, Instruction instruction) {
		int b = Main.pop();
		int a = Main.pop();
		if(a > b)
			Main.ip = Integer.parseInt(instruccion);
		else
			Main.ip++;
	}

}
