/**
 * 
 */
package instrucciones;

import main.Main;
import main.Memoria;
import main.Pila;

/**
 * 
 */
public class SubInstruction implements Instruction {

	@Override
	public void execute(Memoria memoria, Pila pila, Instruction instruction) {
		int b = Main.pop();
		int a = Main.pop();
		Main.push(a - b);
		Main.ip++;
	}

}
