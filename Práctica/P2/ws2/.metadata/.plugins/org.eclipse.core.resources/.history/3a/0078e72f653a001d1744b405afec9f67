 */
package main;

import instructions.*;

/**
 * @author UO285176
 *
 */
public class InstructionParser {

	public static Instruction[] parseInstructions(String[] tokens) {
		Instruction[] instructions = new Instruction[tokens.length];
		int i = 0;
		for (String token : tokens) {
			switch(token) {
			case "push":
				instructions[i] = new Push();
			case "add":
			case "sub":
			case "mul":
			case "jmp":
			case "jmpg":
			case "load":
			case "store":
			case "input":
			case "output":
				
			}
			i++;
		}
	}
}
